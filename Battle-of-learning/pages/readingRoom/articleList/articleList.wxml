<!--pages/readingRoom/articleList/articleList.wxml-->
<view class="article-list">
  <view class="author-img"></view>
  <view class="list-all">
    <view class="list-title word-clamp">1111111111111111</view>
    <view class="list-author">2222222</view>
    <view class="list-content word-clamp">11111111111111111111111111111111111111111111111111111111111111111</view>
    <view class="list-other">
      <view class="list-feedback">
        <view class="feedback-like">
          <image class="like" src="/images/like.png" mode="aspectFit"></image>
          <text class="like-total" space="ensp"> 1   </text>
        </view>
        <view class="feedback-collect">
          <image class="collect" src="/images/collected.png" mode="aspectFit"></image>
          <text class="collect-total" space="ensp"> 2</text>
        </view>
      </view>
      <view class="list-time">
        <text class="time-record" space="ensp">1233333 </text>
        <image class="more" src="/images/more.png" mode="aspectFit"></image>
      </view>
    </view>
  </view>
</view>
